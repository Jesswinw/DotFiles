{
   "author": "Alison Chou",
   "background": {
      "persistent": true,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon128.png",
      "default_popup": "popup.html",
      "default_title": "Clean Up YouTube"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/all-styling.css" ],
      "js": [ "js/content.js" ],
      "matches": [ "https://www.youtube.com/*", "https://m.youtube.com/*" ]
   } ],
   "description": "Hide YouTube related videos, comments, video suggestions wall, homepage recommendations, trending tab, and other distractions.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Remove YouTube Recommended Videos, Comments",
   "offline_enabled": true,
   "permissions": [ "storage", "webRequest", "webRequestBlocking", "https://www.youtube.com/*", "https://m.youtube.com/*" ],
   "short_name": "Clean Up YT",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.4",
   "web_accessible_resources": [ "js/clean-up-youtube.js" ]
}
